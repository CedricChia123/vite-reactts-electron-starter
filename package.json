{
  "name": "super-app",
  "version": "0.5.0",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "Maximilian St√ºmpfl"
  },
  "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/lTimeless/vite-reactts-electron-starter"
  },
  "productName": "super-app",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.18.1",
    "@vitejs/plugin-react": "^1.2.0",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.2.0",
    "electron-dl": "^3.5.1",
    "electron-is-dev": "^2.0.0",
    "electron-windows-badge": "^1.1.0",
    "fuse.js": "^7.0.0",
    "lucide-react": "^0.309.0",
    "node-windows": "^1.0.0-beta.8",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.0",
    "react-router-dom": "^6.21.2",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^2.8.6",
    "windows-elevate": "^1.0.1",
    "yarn": "^1.22.21",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@electron/packager": "^18.1.3",
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^7.0.0",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss": "^8.4.33",
    "prettier": "2.6.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^4.6.2"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
